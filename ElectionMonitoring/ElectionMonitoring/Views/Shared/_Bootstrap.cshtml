<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
	<title>@ViewBag.Title</title>
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<meta name="description" content="DealerBLESS." />
	<meta name="author" content="Ford Credit Europe" />


	<!-- The styles -->
    <link id="bs-css" href="@Url.Content("~/Content/themes/bootstrap/css/bootstrap-main.css")" rel="stylesheet" />
	<style type="text/css">
	  body {
		padding-bottom: 40px;
	  }
	  .sidebar-nav {
		padding: 9px 0;
	  }
	  .alignRight { text-align: right; }
	</style>
    
	<link href='@Url.Content("~/Content/themes/bootstrap/css/bootstrap-responsive.css")' rel="stylesheet" />
	<link href='@Url.Content("~/Content/themes/bootstrap/css/electionmonitoring.css")' rel="stylesheet" />
	<link href='@Url.Content("~/Content/themes/bootstrap/css/jquery-ui-1.8.21.custom.css")' rel="stylesheet" />
	<link href='@Url.Content("~/Content/themes/bootstrap/css/chosen.css")' rel='stylesheet' />
	<link href='@Url.Content("~/Content/themes/bootstrap/css/uniform.default.css")' rel='stylesheet' />
	<link href='@Url.Content("~/Content/themes/bootstrap/css/colorbox.css")' rel='stylesheet' />
	<link href='@Url.Content("~/Content/themes/bootstrap/css/jquery.noty.css")' rel='stylesheet' />
	<link href='@Url.Content("~/Content/themes/bootstrap/css/noty_theme_default.css")' rel='stylesheet' />
	<link href='@Url.Content("~/Content/themes/bootstrap/css/opa-icons.css")' rel='stylesheet' />

	<!-- The HTML5 shim, for IE6-8 support of HTML5 elements -->
	<!--[if lt IE 9]>
	  <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
	<![endif]-->

</head>
<body>
        <!-- topbar starts -->
	<div class="navbar">
		<div class="navbar-inner">
			<div class="container-fluid">
				<a class="btn btn-navbar" data-toggle="collapse" data-target=".top-nav.nav-collapse,.sidebar-nav.nav-collapse">
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
				</a>
				<a class="brand" href="@Url.Action("Dashboard","Misc")"> @*<img alt="Kirk" src="@Url.Content("~/Content/themes/bootstrap/img/logo.png")" />*@ <span>Nigeria Dialogue</span></a>
				
				
				<div class="top-nav nav-collapse">
					<ul class="nav">
						@*<li><a href="#">Log Out</a></li>*@
						<li>
							<form class="navbar-search pull-left">
								<input placeholder="Search" class="search-query span2" name="query" type="text">
							</form>
						</li>
                        <li><a href="#"><i class="icon-info-sign"></i> About </a></li>
                        <li><a href="#"><i class="icon-question-sign"></i> Contact </a></li>
					</ul>
				</div><!--/.nav-collapse -->
			</div>
		</div>
	</div>
	<!-- topbar ends -->
       
		<div class="container-fluid">
		<div class="row-fluid">
				
			<!-- left menu starts -->
			<div class="span1 main-menu-span">
				
			</div><!--/span-->
			<!-- left menu ends -->
			
			<noscript>
				<div class="alert alert-block span10">
					<h4 class="alert-heading">Javascript Disabled</h4>
					<p>Please enable Javascript in your browser  </p>
				</div>
			</noscript>
			
			<div id="content" class="span10">
			    <!-- content starts -->

			    <div>
				    <ul class="breadcrumb">
					    <li>
                            <a class="ajax-link" href="../ElectionResult/RaceResults"><i class="icon-home"></i><span class="hidden-tablet"> <strong>Home</strong> </span></a><span class="divider"> | </span>
					    </li>
                        <li>
						     <a class="ajax-link" href="../ElectionResult/EnterResults"><i class="icon-pencil"></i><span class="hidden-tablet"> <strong>Election Result Entry</strong> </span></a><span class="divider"> | </span>
					    </li>
                        <li>
						     <a class="ajax-link" href="../ElectionResult/Donation"><i class="icon-folder-open"></i><span class="hidden-tablet"> <strong>Donations & Projects</strong> </span></a>
					    </li>
				    </ul>
			    </div>			

			    @RenderBody()<!--/row-->
    
				<!-- content ends -->
			</div><!--/#content.span10-->
		</div><!--/fluid-row-->
				
		<hr>       

        <div class="modal hide fade" id="confirm-dialog">
	        <div class="modal-header">
		        <button type="button" class="close" data-dismiss="modal">×</button>
		        <h3 id='confirm-dialog-title'>Deleting...</h3>
	        </div>
	        <div class="modal-body">
		        <p id='confirm-dialog-message'>Are you sure you want to delete?</p>
	        </div>
	        <div class="modal-footer">
		        <a href="#" class="btn" data-dismiss="modal" >Close</a>
		        <button href="#" class="btn btn-primary" id="confirm-dialog-btn-confirm" onclick =''>Confirm</button>
	        </div>
        </div>


		<footer>
			<p class="pull-left">&copy; <a href="#" target="_blank">Copyright</a> <script>document.write(new Date().getFullYear())</script></p>
			@*<p class="pull-right">Powered by: <a href="#">RoastThatMeat</a></p>*@
		</footer>
		
	</div><!--/.fluid-container-->

    <!-- jQuery -->
	<script type="text/javascript" src="@Url.Content("~/Content/themes/bootstrap/js/jquery-1.7.2.min.js")"></script>
    @*<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>*@
	<!-- jQuery UI -->
	<script type="text/javascript" src="@Url.Content("~/Content/themes/bootstrap/js/jquery-ui-1.8.21.custom.min.js")"></script>    
    <!-- jQuery Ajax -->
    @Scripts.Render("~/Scripts/jquery.unobtrusive-ajax.min.js")
    <!-- jQuery Validation -->
    @Scripts.Render("~/Scripts/jquery.validate.min.js")
	<!-- transition / effect library -->
	<script type="text/javascript" src="@Url.Content("~/Content/themes/bootstrap/js/bootstrap-transition.js")"></script>
	<!-- alert enhancer library -->
	<script type="text/javascript" src="@Url.Content("~/Content/themes/bootstrap/js/bootstrap-alert.js")"></script>
	<!-- modal / dialog library -->
	<script type="text/javascript" src="@Url.Content("~/Content/themes/bootstrap/js/bootstrap-modal.js")"></script>
	<!-- custom dropdown library -->
	<script type="text/javascript" src="@Url.Content("~/Content/themes/bootstrap/js/bootstrap-dropdown.js")"></script>
	<!-- scrolspy library -->
	<script type="text/javascript" src="@Url.Content("~/Content/themes/bootstrap/js/bootstrap-scrollspy.js")"></script>
	<!-- library for creating tabs -->
	<script type="text/javascript" src="@Url.Content("~/Content/themes/bootstrap/js/bootstrap-tab.js")"></script>
	<!-- library for advanced tooltip -->
	<script type="text/javascript" src="@Url.Content("~/Content/themes/bootstrap/js/bootstrap-tooltip.js")"></script>
	<!-- popover effect library -->
	<script type="text/javascript" src="@Url.Content("~/Content/themes/bootstrap/js/bootstrap-popover.js")"></script>
	<!-- button enhancer library -->
	<script type="text/javascript" src="@Url.Content("~/Content/themes/bootstrap/js/bootstrap-button.js")"></script>
    <!-- select or dropdown enhancer -->
	<script type="text/javascript" src="@Url.Content("~/Content/themes/bootstrap/js/jquery.chosen.min.js")"></script>
	<!-- checkbox, radio, and file input styler -->
	<script type="text/javascript" src="@Url.Content("~/Content/themes/bootstrap/js/jquery.uniform.min.js")"></script>
	<!-- notification plugin -->
	<script type="text/javascript" src="@Url.Content("~/Content/themes/bootstrap/js/jquery.noty.js")"></script>
    <!-- data table plugin -->
	<script type="text/javascript" src='@Url.Content("~/Content/themes/bootstrap/js/jquery.dataTables.min.js")'></script>
	<!-- library for cookie management -->
	<script type="text/javascript" src="@Url.Content("~/Content/themes/bootstrap/js/jquery.cookie.js")"></script>

    
    <!-- application script for electionmonitoring -->
	<script type="text/javascript" src="@Url.Content("~/Content/themes/bootstrap/js/electionmonitoring.js")"></script>
    
	<!-- chart libraries start -->
    <script src="@Url.Content("~/Scripts/highcharts/highcharts.js")"></script>
    <script src="@Url.Content("~/Scripts/highcharts/modules/exporting.js")"></script>
    <!-- chart libraries end -->

    <!-- Google MAP API -->
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?v=3.exp&sensor=false"></script>

    @RenderSection("scripts", required: false)
</body>
</html>
